access_rules = {
  days                 = ["Monday", "Tuesday"]
  from_time            = "09:00"
  to_time              = "17:00"
  max_session_duration = 8
  time_zone            = "America/New_York"
}

aws_region        = "us-east-2"
aws_profile       = "default"
secret_name       = "my-aws-secret-name"
